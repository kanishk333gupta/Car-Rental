<br>
<table class = "table table-striped table-bordered table-hover table-sm">
  <thead class="thead-dark">
    <tr>
      <th>UserName</th>
      <th>Email</th>
      <th>Status</th>
      <th>Show</th>
      <th>del</th>
    </tr>
  </thead>
  <% @users.each do |user|  %>
    <tbody>
    <tr>
      <td><%= user.email.split('@')[0]%></td>
      <td><%= user.email %></td>
      <td>
        <%if user.admin==true%>
          <%="ADMIN"%>
        <%else%>
          <%= "Customer" %>
        <%end%>
      </td>
      <td><%= link_to "Show this user", user %></td>
      <td><%= link_to "Delete", user , data: { "turbo-method": :delete, confirm: "del?" }, class: "btn btn-danger btn-sm" %>
          </td>
    </tr>

    <% end %>
    </tbody>
</table>

<%= link_to "Booking" , '/home/booking' %>